  "selectors": {
    "usernameInput": {
      "by": "getByLabel",
      "value": "Username or email"
    },
    "passwordInput": {
      "by": "locator",
      "value": "input[type='password']#password"
    },
    "loginButton": {
      "by": "getByRole",
