    },
    "clientsButton": {
      "by": "getByTestId",
      "value": "nav-item-clients"
    },
    "nextcloudClientLink": {
      "by": "locator",
      "value": "a[href='#/opnc/clients/d97b00f4-c4eb-41f0-ad9e-0c6674d13348/settings']"
