{
  "url": "https://nextcloud.test",
  "selectors": {
    "usernameInput": {
      "by": "locator",
      "value": "#user"
    },
    "passwordInput": {
      "by": "locator",
      "value": "#password"
    },
    "loginButton": {
      "by": "locator",
      "value": "button[data-login-form-submit][type='submit']"
    },
    "welcomeMessageClose": {
      "by": "locator",
      "value": ".welcome .close, .modal__close, [aria-label*='close'], [aria-label*='Close']"
    },
    "profileIcon": {
      "by": "locator",
      "value": "button[aria-label='Settings menu']"
    },
    "administrationSettingsLink": {
      "by": "locator",
      "value": "a#admin_settings[href*='/settings/admin/overview']"
    },
    "openIdConnectLink": {
      "by": "locator",
      "value": "li[data-section-id='user_oidc'] a[href*='/settings/admin/user_oidc']"
    },
    "keycloakProviderDetails": {
      "by": "locator",
      "value": ".oidcproviders__details"
    },
    "keycloakProviderName": {
      "by": "locator",
      "value": ".oidcproviders__details h3:has-text('keycloak')"
    },
    "keycloakClientId": {
      "by": "locator",
      "value": ".oidcproviders__details label:has-text('Client ID') + span"
    },
    "keycloakDiscoveryEndpoint": {
      "by": "locator",
      "value": ".oidcproviders__details label:has-text('Discovery endpoint') + span"
    },
    "keycloakBackchannelLogoutUrl": {
      "by": "locator",
      "value": ".oidcproviders__details label:has-text('Backchannel Logout URL') + span"
    },
    "keycloakRedirectUri": {
      "by": "locator",
      "value": ".oidcproviders__details label:has-text('Redirect URI') + span"
    },
    "userMenu": {
      "by": "locator",
      "value": ".header-menu, [data-user-menu], #user-menu, .avatardiv"
    },
    "integrationAppTitle": {
      "by": "locator",
      "value": "h2:has-text('OpenProject')"
    },
    "integrationAppLink": {
      "by": "getByRole",
      "value": {
        "role": "link",
        "name": "OpenProject"
      }
    },
    "appsMenu": {
      "by": "locator",
      "value": "a#core_apps[href='/settings/apps']"
    },
    "activeAppsLink": {
      "by": "locator",
      "value": "a.app-navigation-entry-link[href='/settings/apps/enabled'][title='Active apps']"
    },
    "openProjectIntegrationAppRow": {
      "by": "locator",
      "value": "tr.app-item:has(a.app-name--link[href*='integration_openproject'])"
    },
    "openProjectIntegrationAppLink": {
      "by": "locator",
      "value": "a.app-name--link[href*='integration_openproject']:has-text('OpenProject Integration')"
    },
    "openProjectIntegrationAppVersion": {
      "by": "locator",
      "value": "tr.app-item:has(a.app-name--link[href*='integration_openproject']) td.app-version span"
    },
    "openProjectIntegrationDisableButton": {
      "by": "locator",
      "value": "tr.app-item:has(a.app-name--link[href*='integration_openproject']) button:has-text('Disable')"
    }
  }
}

